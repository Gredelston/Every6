<script>
	$(document).ready(function() {
    console.log("Firing get request");
		$.get('/getGoogleUser', function(data) {
      console.log("DATA: ", data);

      if (data) {
        $.get('/loggedInDisplay', function(renderedHTML) {
          $("#navbar-auth").html(renderedHTML);
        });
      } else {
        $("#navbar-auth").html(
          "<img src='images/google-login.png' height='26' />"
        );
      }
		});
	});
</script>