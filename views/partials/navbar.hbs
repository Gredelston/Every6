<script>
	$(document).ready(function() {
    $.get('/getGoogleUser', function(data) {
      
      if (data) {
        $.get('/loggedInDisplay', function(renderedHTML) {
          $("#navbar-profile").html(renderedHTML);
        });

      } else {
        $("#navbar-profile").html(
          "<a href='/auth'><img src='/images/google-login.png' id='login-image' height='35' /></a>"
        );
      }

		});
	});
</script>